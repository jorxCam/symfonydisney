        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>
    <body>
     <header>  
        <nav class="navbar">
            <div class="logo">
                            <a href="#" class="active hidden logodisney">
